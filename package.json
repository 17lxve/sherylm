{
  "name": "sheryl",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/ && prettier --write backend/src/",
    "serve": "vite --host --open",
    "dist": "vite build && git add dist -f && git commit -m 'auto-update' && git subtree push --prefix dist origin gh-pages",
    "start": "cd backend && npm run dev",
    "full": "concurrently \"vite --host --open\" \"cd backend && npm run dev\""
  },
  "dependencies": {
    "axios": "^1.5.0",
    "concurrently": "^8.2.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "prettier": "^3.0.0",
    "vite": "^4.4.6"
  }
}
